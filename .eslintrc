{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "__SERVER__": true,
    "__CLIENT__": true,
    "__TEST__": true,
    "__DEV__": true,
    "__PROD__": true,
    "__STAGING__": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "jsx": true
  },
  "parser": "babel-eslint",
  "rules": {
    "prefer-reflect": [
      0,
      {
        "exceptions": [
          "apply",
          "call",
          "delete"
        ]
      }
    ],
    "babel/new-cap": 2,
    "no-return-assign": 2,
    "no-invalid-this": 0,
    "no-void": 2,
    "one-var": [2, "never"],
    "react/jsx-closing-bracket-location": 0,
    "no-undef": 2,
    "max-nested-callbacks": [
      2,
      3
    ],
    "no-empty": 2,
    "no-loop-func": 2,
    "keyword-spacing": 2,
    "babel/object-shorthand": [
      2,
      "always"
    ],
    "wrap-iife": [
      2,
      "inside"
    ],
    "valid-typeof": 2,
    "react/jsx-no-literals": 2,
    "handle-callback-err": 2,
    "operator-linebreak": [2, "after"],
    "no-label-var": 2,
    "no-process-env": 2,
    "no-irregular-whitespace": 2,
    "block-spacing": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "react/jsx-pascal-case": 2,
    "no-empty-pattern": 2,
    "radix": 2,
    "no-undefined": 0,
    "semi-spacing": 2,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "no-negated-condition": 2,
    "require-yield": 2,
    "new-cap": 2,
    "no-const-assign": 2,
    "no-bitwise": 2,
    "dot-notation": 2,
    "camelcase": 2,
    "prefer-const": 2,
    "no-negated-in-lhs": 2,
    "prefer-arrow-callback": 2,
    "no-extra-bind": 2,
    "react/prefer-es6-class": 2,
    "no-sequences": 2,
    "babel/generator-star-spacing": 2,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "no-spaced-func": 2,
    "react/require-extension": 2,
    "no-labels": 2,
    "no-unreachable": 2,
    "no-eval": 2,
    "react/no-did-mount-set-state": 2,
    "no-unneeded-ternary": 2,
    "no-process-exit": 2,
    "no-empty-character-class": 2,
    "constructor-super": 2,
    "no-dupe-class-members": 2,
    "strict": [
      2,
      "never"
    ],
    "no-case-declarations": 2,
    "array-bracket-spacing": 2,
    "react/no-set-state": 2,
    "block-scoped-var": 2,
    "arrow-body-style": 2,
    "space-in-parens": 2,
    "no-confusing-arrow": 2,
    "no-control-regex": 2,
    "consistent-return": 2,
    "no-console": 2,
    "comma-spacing": 2,
    "no-redeclare": 2,
    "computed-property-spacing": 2,
    "no-invalid-regexp": 2,
    "use-isnan": 2,
    "no-new-require": 2,
    "indent": [
      2,
      2
    ],
    "react/react-in-jsx-scope": 2,
    "no-native-reassign": 2,
    "no-func-assign": 2,
    "max-len": [
      2,
      120,
      4,
      {
        "ignoreUrls": true
      }
    ],
    "no-shadow": [
      2,
      {
        "builtinGlobals": true
      }
    ],
    "no-mixed-requires": 2,
    "react/no-did-update-set-state": 2,
    "react/jsx-uses-react": 2,
    "max-statements": [
      2,
      20
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "no-lone-blocks": 2,
    "no-debugger": 2,
    "arrow-parens": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "no-implied-eval": 2,
    "no-useless-concat": 2,
    "no-multi-spaces": 2,
    "curly": [2, "multi-line"],
    "no-extra-boolean-cast": 2,
    "space-infix-ops": 2,
    "babel/no-await-in-loop": 2,
    "react/sort-comp": 2,
    "react/jsx-no-undef": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "semi": 2,
    "no-param-reassign": 0,
    "no-cond-assign": 2,
    "no-dupe-keys": 2,
    "import/named": 0,
    "max-params": [
      2,
      4
    ],
    "linebreak-style": 2,
    "react/jsx-sort-props": [
      0,
      {
        "shorthandFirst": true,
        "callbacksLast": true
      }
    ],
    "no-octal-escape": 2,
    "no-this-before-super": 2,
    "no-alert": 2,
    "react/jsx-no-duplicate-props": [
      2,
      {
        "ignoreCase": true
      }
    ],
    "no-unused-expressions": 2,
    "react/jsx-sort-prop-types": 0,
    "no-class-assign": 2,
    "spaced-comment": 2,
    "no-path-concat": 2,
    "prefer-spread": 2,
    "no-self-compare": 2,
    "guard-for-in": 2,
    "no-nested-ternary": 2,
    "no-multi-str": 2,
    "react/jsx-key": 1,
    "import/namespace": 2,
    "no-warning-comments": 1,
    "no-delete-var": 2,
    "babel/arrow-parens": [
      2,
      "always"
    ],
    "no-with": 2,
    "no-extra-parens": 2,
    "no-trailing-spaces": 2,
    "import/no-unresolved": 1,
    "no-obj-calls": 2,
    "accessor-pairs": 2,
    "yoda": [
      2,
      "never",
      {
        "exceptRange": true
      }
    ],
    "no-continue": 1,
    "react/no-unknown-property": 2,
    "no-new": 2,
    "object-curly-spacing": 2,
    "react/jsx-curly-spacing": [
      2,
      "never"
    ],
    "jsx-quotes": 2,
    "react/no-direct-mutation-state": 2,
    "key-spacing": 2,
    "no-underscore-dangle": [
      2,
      { "allowAfterThis": true }
    ],
    "new-parens": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-floating-decimal": 2,
    "operator-assignment": [
      2,
      "always"
    ],
    "no-shadow-restricted-names": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-useless-call": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "react/jsx-handler-names": [
      1,
      {
        "eventHandlerPrefix": "handle",
        "eventHandlerPropPrefix": "on"
      }
    ],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "no-unused-vars": 2,
    "import/default": 1,
    "no-lonely-if": 2,
    "no-extra-semi": 2,
    "prefer-template": 2,
    "react/forbid-prop-types": 1,
    "react/self-closing-comp": 2,
    "no-else-return": 2,
    "react/jsx-max-props-per-line": [
      2,
      {
        "maximum": 3
      }
    ],
    "no-dupe-args": 2,
    "no-new-object": 2,
    "callback-return": 2,
    "no-new-wrappers": 2,
    "comma-style": 2,
    "no-script-url": 2,
    "consistent-this": 2,
    "react/wrap-multilines": 0,
    "dot-location": [
      2,
      "property"
    ],
    "no-implicit-coercion": 2,
    "max-depth": [
      2,
      4
    ],
    "babel/object-curly-spacing": [
      2,
      "never"
    ],
    "no-array-constructor": 2,
    "no-iterator": 2,
    "react/jsx-no-bind": 2,
    "sort-vars": 2,
    "no-var": 2,
    "no-sparse-arrays": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "no-throw-literal": 2,
    "no-proto": 2,
    "default-case": 2,
    "no-inner-declarations": 2,
    "react/jsx-indent-props": [
      2,
      2
    ],
    "no-new-func": 2,
    "object-shorthand": 2,
    "no-ex-assign": 2,
    "no-unexpected-multiline": 2,
    "no-undef-init": 2,
    "no-duplicate-case": 2,
    "no-fallthrough": 2,
    "no-catch-shadow": 2,
    "import/export": 2,
    "no-constant-condition": 2,
    "complexity": [
      2,
      25
    ],
    "react/jsx-boolean-value": [
      2,
      "never"
    ],
    "valid-jsdoc": 2,
    "no-extend-native": 2,
    "react/prop-types": 2,
    "no-regex-spaces": 2,
    "react/no-multi-comp": 2,
    "no-octal": 2,
    "arrow-spacing": 2,
    "quote-props": [
      2,
      "as-needed"
    ],
    "no-div-regex": 2,
    "react/jsx-uses-vars": 2,
    "react/no-danger": 1
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true,
    "import/parser": "babel-eslint",
    "import/ignore": [
      "node_modules",
      "\\.scss$"
    ],
    "import/resolve": {
      "moduleDirectory": [
        "node_modules"
      ]
    }
  },
  "plugins": [
    "react",
    "import",
    "babel"
  ]
}
